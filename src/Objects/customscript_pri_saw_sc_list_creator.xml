<scheduledscript scriptid="customscript_pri_saw_sc_list_creator">
  <description>Nightly scheduled script to create lists for options in SAW.</description>
  <isinactive>F</isinactive>
  <name>SAW List Creator</name>
  <notifyadmins>F</notifyadmins>
  <notifyemails></notifyemails>
  <notifyowner>T</notifyowner>
  <scriptfile>[/SuiteScripts/SAW/Server/SC_CreateLists.js]</scriptfile>
  <scriptdeployments>
    <scriptdeployment scriptid="customdeploy_pri_saw_sc_list_creator2">
      <isdeployed>T</isdeployed>
      <loglevel>DEBUG</loglevel>
      <status>NOTSCHEDULED</status>
      <title>SAW List Creator 2</title>
      <recurrence>
        <single>
          <repeat></repeat>
          <startdate>2020-05-12</startdate>
          <starttime>01:00:00Z</starttime>
        </single>
      </recurrence>
    </scriptdeployment>
    <scriptdeployment scriptid="customdeploy_pri_saw_sc_list_creator">
      <isdeployed>T</isdeployed>
      <loglevel>DEBUG</loglevel>
      <status>SCHEDULED</status>
      <title>SAW List Creator</title>
      <recurrence>
        <daily>
          <everyxdays>1</everyxdays>
          <repeat></repeat>
          <startdate>2020-05-12</startdate>
          <starttime>09:00:00Z</starttime>
        </daily>
      </recurrence>
    </scriptdeployment>
  </scriptdeployments>
</scheduledscript>